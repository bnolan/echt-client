{
  "name": "echt-server",
  "version": "0.0.1",
  "description": "Scripts for serverless operation of the echt app",
  "main": "index.js",
  "scripts": {
    "start": "claudia create --version dev --region us-west-2 --api-module app --name echt",
    "release-dev": "claudia update --version dev",
    "release-uat": "claudia update --version uat",
    "release-prod": "claudia update --version prod",
    "test": "tape tests/*-test.js | faucet",
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnolan/echt.git"
  },
  "keywords": [
    "lambda",
    "javascript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bnolan/echt/issues"
  },
  "homepage": "https://github.com/bnolan/echt#readme",
  "dependencies": {
    "aws-sdk": "^2.27.0",
    "claudia-api-builder": "^2.4.1",
    "jsonwebtoken": "^7.3.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "claudia": "^2.9.0",
    "faucet": "^0.0.1",
    "proxyquire": "^1.7.11",
    "semistandard": "^10.0.0",
    "sinon": "^2.0.0",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6"
  }
}
