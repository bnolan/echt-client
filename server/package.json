  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bnolan/echt/issues"
  },
  "homepage": "https://github.com/bnolan/echt#readme",
  "dependencies": {
    "aws-sdk": "^2.27.0",
    "claudia-api-builder": "^2.4.1"
  },
  "devDependencies": {
    "claudia": "^2.9.0",
    "proxyquire": "^1.7.11",
    "semistandard": "^10.0.0",
    "sinon": "^2.0.0",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6"
  }
}
