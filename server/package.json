{
  "name": "echt-server",
  "version": "0.0.1",
  "description": "Scripts for serverless operation of the echt app",
  "main": "index.js",
  "scripts": {
    "start": "claudia create --version dev --region us-west-2 --api-module app --name echt",
    "server": "nodemon local-server.js",
    "release-dev": "claudia update --version dev --use-s3-bucket echt-deploys.us-west-2",
    "release-uat": "claudia update --version uat --use-s3-bucket echt-deploys.us-west-2",
    "release-prod": "claudia update --version prod --use-s3-bucket echt-deploys.us-west-2",
    "test": "tape -r ./tests/bootstrap tests/**/*-test.js",
    "test-unit": "tape -r ./tests/bootstrap tests/unit/*-test.js",
    "test-end-to-end": "tape -r ./tests/bootstrap tests/end-to-end/*-test.js",
    "lint": "semistandard",
    "logs": "open https://us-west-2.console.aws.amazon.com/cloudwatch/home?region=us-west-2#logEventViewer:group=/aws/lambda/echt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnolan/echt.git"
  },
  "keywords": [
    "lambda",
    "javascript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bnolan/echt/issues"
  },
  "homepage": "https://github.com/bnolan/echt#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.27.0",
    "aws-signature-v4": "^1.0.1",
    "claudia-api-builder": "^2.4.1",
    "es6-promisify": "^5.0.0",
    "imagemagick": "^0.1.3",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "raygun": "^0.9.1",
    "tempfile": "^1.1.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "body-parser": "^1.17.1",
    "claudia": "^2.9.0",
    "express": "^4.15.2",
    "faucet": "^0.0.1",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "semistandard": "^10.0.0",
    "sinon": "^2.0.0",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "yargs": "^7.0.2"
  }
}
