{
  "name": "echt-server",
  "version": "0.0.1",
  "description": "Scripts for serverless operation of the echt app",
  "main": "index.js",
  "scripts": {
    "deploy": "claudia update --version dev --region ap-southeast-2 --api-module api --name test --use-s3-bucket echt-deploys",
    "release": "claudia set-version --version production",
    "test": "tape tests/* | faucet",
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnolan/echt.git"
  },
  "keywords": [
    "lambda",
    "javascript"
  ],
